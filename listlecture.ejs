<html>
    <body>
        <h1>Lecturers</h1>
        <br>
        <button><a href="/addLecturer">Add Lecturer</a></button>
        <br></br>
<table rules="all">
    <tr>
        <th>Lecturer ID</th>
        <th>Name</th>
        <th>Dept</th>
    </tr>
    <% lecturers.forEach((lecturer) => { %>
        <tr>
            <td><%= lecturer._id %></td>
            <td><%= lecturer.name %></td>
            <td><%= lecturer.dept %></td>
        </tr>
        <% }) %>
</table>
<a href="/">Home</a>
</body>
</html>