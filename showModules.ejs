<html>
    <body>
        <h1>Modules</h1>
        <br>

<table rules="all">
    <tr>
        <th>Module ID</th>
        <th>Name</th>
        <th>Credits</th>
        <th>Update</th>
        <th>Students</th>
    </tr>
    <% modules.forEach((module) => { %>
        <tr>
            <td><%= module.mid %></td>
            <td><%= module.name %></td>
            <td><%= module.credits %></td>
            <td><a href="/edit/<%= module.mid %>">Update</a></td>
            <td><a href="/students/<%= module.mid %>">List</a></td>
        </tr>
        <% }) %>
</table>
<a href="/">Home</a>
</body>
</html>